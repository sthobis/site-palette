{
    "name": "site-palette",
    "alias": "site-palette",
    "version": 2,
    "builds": [
        { "src": "www/package.json", "use": "@now/next" },
        { "src": "api/*.js", "use": "@now/node", "config": { "maxLambdaSize": "40mb" } }
    ],
    "routes": [
        {
          "src": "/api/get-palette.js",
          "methods": ["GET"]
        },
        {
          "src": "/(.*)",
          "dest": "/www/$1"
        }
    ]
}
